  "files": [
    "index.js"
  ],
  "dependencies": {
    "@mapbox/hast-util-table-cell-style": "^0.1.3",
    "hast-to-hyperscript": "^8.0.0"
  },
  "devDependencies": {
    "hastscript": "^5.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "remark": "^11.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.0",
    "tape": "^4.0.0",
    "unified": "^9.0.0",
    "unist-builder": "^2.0.0",
    "xo": "^0.28.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
